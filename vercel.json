{
    "version": 2,
    "builds": [
      { "src": "/index.py", "use": "@vercel/python" }
    ],
    "routes": [
      { "src": "/.*", "dest": "/index.py" }
    ],
    "crons": [
        {
            "path": "/app/daily_text.py",
            "schedule": "0 17 * * *",
            "use": "@vercel/python"
        },
        {
            "path": "/app/backup_database.py",
            "schedule": "10 8 * * *",
            "use": "@vercel/python"
        }
    ]
}